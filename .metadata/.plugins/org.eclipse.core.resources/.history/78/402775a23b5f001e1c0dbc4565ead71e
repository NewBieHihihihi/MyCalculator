package view;

import javax.swing.JFrame;

public class MyWindow extends JFrame{
	public MyWindow()
	{
		
	}
	public void show()//cho phép hiển thị cửa sổ
	{
		this.setVisible(true);
	}
    //@over_loading
	public void show(String title)//cho phép hiển thị với tên tiêu đề
	{
		this.setTitle(title);
	}

}
